@using WebAppMVCLesson1.NewAdmin.Modals
@model List<RoomProperty> 

<table>
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
        </tr>
    </thead>
    <tbody>
        @foreach(RoomProperty item in Model)
        {
            <tr>
                <td>@item.PropertyId</td>
                <td>@item.Name</td>
                <td><a asp-action="Edit" asp-controller="RoomProperty" asp-route-id="@item.PropertyId">Edit</a></td>
                <td><a asp-action="Delete" asp-controller="RoomProperty" asp-route-id="@item.PropertyId">Delete</a></td>
            </tr>
        }
    </tbody>
</table>
<hr />
<a asp-action="" asp-controller="">Add</a>
